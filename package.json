{
  "name": "chat",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.1",
    "react-loadable": "^5.4.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "styled-components": "^3.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "connect-history-api-fallback": "^1.5.0",
    "dotenv-safe": "^6.0.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-fbjs": "^2.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "eslint-plugin-relay": "^0.0.21",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.74.0",
    "flow-typed": "^2.4.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "koa-connect": "^2.0.1",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.5",
    "react-test-renderer": "^16.4.0",
    "rimraf": "^2.6.2",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-serve": "^1.0.4",
    "webpack-serve-waitpage": "^1.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/coverage/",
      "<rootDir>/node_modules/"
    ],
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "/__tests__/",
      "/coverage/",
      "/node_modules/",
      "<rootDir>/test"
    ],
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$": "<rootDir>/test/__mocks__/fileMock.js"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn prettier",
      "eslint --fix",
      "stylelint",
      "git add"
    ]
  },
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "yarn lint:staged",
    "lint": "eslint --ext js src",
    "lint:css": "stylelint './src/**/*.js'",
    "lint:staged": "lint-staged",
    "prettier": "prettier --write --single-quote true --trailing-comma all --print-width 120",
    "flow": "flow check",
    "start": "webpack-serve --config webpack.config.js",
    "build": "cp .env.prod .env && rimraf ./build && webpack -p --config webpack.prod.config.js --progress --profile --colors",
    "build:dev": "cp .env.staging .env && rimraf ./build && webpack -p --config webpack.config.js --progress --profile --colors",
    "test": "jest --runInBand --forceExit",
    "test:watch": "yarn test --watch",
    "config:staging": "cp .env.staging .env",
    "config:prod": "cp .env.prod .env"
  }
}
